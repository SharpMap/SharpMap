  </runtime>
  <applicationSettings>
    <WinFormSamples.Properties.Settings>
      <setting name="OleDbProvider" serializeAs="String">
        <value>Microsoft.ACE.OLEDB.15.0</value>
      </setting>
    </WinFormSamples.Properties.Settings>
  </applicationSettings>
</configuration>